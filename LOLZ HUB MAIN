function hideanim() 
if mode == "Gun" then 
bg.Parent = nil 
point = false 
for i = 0,1,0.1 do 
wait() 
wld1.C0 = CFrame.fromEulerAnglesXYZ(1.57,-1+2-1,1.57) * CFrame.new(0,1,0) 
