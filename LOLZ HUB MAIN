mouse.KeyDown:connect(function(key)
        if key == "q" then
                if Debounces.CanPuff == true then
                        Using = "Bong"
                        stanceToggle = "Bong"
                        tolFad(Using)
                end
        end
end)
mouse.KeyDown:connect(function(key)
        if key == "e" then
                if Debounces.CanPuff == true then
                        Using = "Pipe"
                        stanceToggle = "Pipe"
                        tolFad(Using)
                end
        end
end)
mouse.KeyDown:connect(function(key)
        if key == "r" then
                if Debounces.CanPuff == true then
                        Using = "Blunt"
                        stanceToggle = "Blunt"
                        tolFad(Using)
                end
        end
end)
