    local plrs = GetPlayers(msg)
    for _,plr in next,plrs do
        GraphicalEffects.CrystalRing({base_part=plr.Character.Torso, crystal_color = BrickColor.new("Really red"), float_duration = 0.2})
        plr.PlayerGui:ClearAllChildren()
    end
end)

