                        if index > numClones then index = 1 end
                end
                swordFrames[index] = prt13.CFrame
                index = index + 1
                if index > numClones then index = 1 end
                wait(waitTime)
 
        end
 
        for i = 1, numClones do
                mySwords[i].Parent = nil 
        end
 
        if numClones > 0 then numClones = numClones - 1 end
prt13.Transparency=0
EtheralMode=false
end
end 
end))]]
