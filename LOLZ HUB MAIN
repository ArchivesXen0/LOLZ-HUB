                p.P=3000
                p.D=100
                p.maxForce=Vector3.new(math.huge,0,math.huge)
                p.position=Torsoh2.Position
                p.Parent=Torsoh2]]
                while part.Parent ~= nil and lightning == true do 
--                        f1:Play()
--                        p.position=ST2+Vector3.new(math.random(-50,50)/50,0,math.random(-50,50)/50)
                        Start2=End2
