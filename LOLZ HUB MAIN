        Handleweld.C0=clerp(Handleweld.C0,cf(0,20,-5)*angles(math.rad(150),math.rad(0),math.rad(0)),.2)
        Handle2weld.C0=clerp(Handle2weld.C0,cf(0,20,-5)*angles(math.rad(90),math.rad(0),math.rad(0)),.2)
