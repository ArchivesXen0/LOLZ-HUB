for x=1,30 do
wait();
rm.Scale=rm.Scale:lerp(Vector3.new(i*30,i*30,(4-i)*450),.1);
e.Transparency=x/30+.5;
end;
end);
end;
c.HumanoidRootPart.Velocity=c.HumanoidRootPart.CFrame.lookVector*200;
end;
end);
